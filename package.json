{
  "name": "mcp-appstore-connect",
  "version": "1.2.2",
  "description": "MCP server for Apple App Store Connect API",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "mcp-appstore-connect": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahmetsina/mcp-appstore-connect.git"
  },
  "bugs": {
    "url": "https://github.com/ahmetsina/mcp-appstore-connect/issues"
  },
  "homepage": "https://github.com/ahmetsina/mcp-appstore-connect#readme",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "prepublishOnly": "npm run build && npm run test:run",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\""
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "appstore",
    "app-store-connect",
    "apple",
    "ios",
    "testflight"
  ],
  "author": "Ahmet Sina Ustem <asend.ustem@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.0",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitest/ui": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "prettier": "^3.8.1",
    "tsx": "^4.15.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.53.1",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
